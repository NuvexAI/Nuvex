# Application Settings
APP_NAME=deepcore
HOST=0.0.0.0
PORT=8080
LOG_LEVEL=INFO

# OpenTelemetry Configuration
OTEL_ENABLED=true
OTEL_TRACE_UPLOAD_ENABLED=false
OTEL_TRACE_UPLOAD_URL=http://127.0.0.1:4318/v1/traces

# API Security
API_KEYS=["your-api-key-1","your-api-key-2"]  # List of allowed API keys
API_KEY=your-default-api-key
JWT_SECRET=your-jwt-secret-key
ACCESS_TOKEN_EXPIRE_MINUTES=720
REFRESH_TOKEN_EXPIRE_DAYS=30
ENCRYPTION_KEY=your-encryption-key

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1  # OpenAI API base URL
MODEL_NAME=gpt-4o-2024-11-20

# External Services
TWITTER_TOKEN=your-twitter-token

# Coin Service Configuration
COIN_HOST=your-coin-host-url
COIN_HOST_V2=your-coin-host-v2-url
COIN_API_KEY=your-coin-api-key
COIN_API_KEY_V2=your-coin-api-v2-key

# AI Search Configuration
AI_SEARCH_HOST=your-ai-search-host
AI_SEARCH_KEY=your-ai-search-key

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0
REDIS_PREFIX=default

# MySQL Configuration
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=mydatabase

# OpenAPI Configuration
OPENAPI_FITTER_FIELDS=[]  # List of fields to filter in OpenAPI documentation

# Data API Configuration
DATA_API_KEY=your_data_api_key
DATA_API_BASE=https://api.example.com
